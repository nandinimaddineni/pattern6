public class Main{
    public static void main(String[] args){
        int n=5;
        for(int rows=1;rows<=n;rows++){
            for(int s=0;s<=n-rows;s++){
                System.out.print(" ");
            }
            for(int cols=1;cols<2*rows-1;cols++){
            System.out.print(cols);
        }
        System.out.println();
        }
    
    }
}
